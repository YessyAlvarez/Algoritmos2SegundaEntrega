	}

	public void crearVertice( Comparable palabra) {
		if(this.nodosGrafo.existeNodoPalabra(palabra)) {
			
		}
		
	}
	
	
	
	private boolean existeNodoPalabra(Comparable palabra) {
		NodoGrafo first = this.primero;
		return buscarRecursivo(first, unaEtiqueta);
		return false;
	}
	
	
	
	
	
	
	
